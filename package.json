{
  "name": "jbq",
  "version": "0.1.1",
  "description": "JBQ - Yet another customizable JS schema validator.",
  "main": "main.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "wiki": "node --require ts-node/register ./md/create.ts",
    "build": "npm run tsc:esm && npm run tsc:cjs && node --require ts-node/register ./build.ts",
    "tsc:cjs": "node_modules/.bin/tsc -p ./tsconfig.build_cjs.json",
    "tsc:esm": "node_modules/.bin/tsc -p ./tsconfig.build_esm.json",
    "tsc:dev": "node_modules/.bin/tsc -p ./tsconfig.dev.json",
    "tsc:dev:watch": "node_modules/.bin/tsc -w -p ./tsconfig.dev.json",
    "test": "mocha --require ts-node/register ./test/main.test.ts",
    "test:cov": "c8 mocha --require ts-node/register ./test/main.test.ts",
    "test:perf": "node --require ts-node/register ./test/perf/benchmark.ts",
    "lint": "eslint src/**/*.ts test/**/*.ts md/**/*.ts --fix"
  },
  "keywords": [
    "validator"
  ],
  "author": "krnik",
  "license": "ISC",
  "homepage": "https://github.com/krnik/jbq",
  "repository": {
    "type": "git",
    "url": "https://github.com/krnik/jbq"
  },
  "devDependencies": {
    "@types/benchmark": "1.0.31",
    "@types/chai": "4.1.7",
    "@types/faker": "4.1.5",
    "@types/mocha": "5.2.6",
    "@types/node": "11.13.7",
    "@types/prettier": "1.16.3",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "@typescript-eslint/parser": "1.7.0",
    "benchmark": "2.1.4",
    "c8": "3.5.0",
    "chai": "4.2.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-prettier": "3.0.1",
    "faker": "4.1.0",
    "mocha": "6.1.4",
    "prettier": "1.17.0",
    "testcheck": "1.0.0-rc.2",
    "ts-node": "8.1.0",
    "typescript": "3.4.5"
  },
  "dependencies": {}
}
