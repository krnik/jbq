{
  "name": "vjs-validator",
  "version": "0.0.2",
  "description": "Yet another customizable JS schema validator.",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "wiki": "node ./md/create.js",
    "tsc": "node_modules/.bin/tsc -p ./tsconfig.build.json",
    "tsc:dev": "node_modules/.bin/tsc -p ./tsconfig.dev.json",
    "tsc:dev:watch": "node_modules/.bin/tsc -w -p ./tsconfig.dev.json",
    "test": "mocha --require ts-node/register ./test/main.test.ts",
    "test:cov": "c8 mocha --require ts-node/register ./test/main.test.ts",
    "test:perf": "node --require ts-node/register ./test/perf/benchmark.ts"
  },
  "keywords": [
    "validator"
  ],
  "author": "krnik",
  "license": "ISC",
  "homepage": "https://github.com/krnik/vjs-validator",
  "repository": {
    "type": "git",
    "url": "https://github.com/krnik/vjs-validator"
  },
  "devDependencies": {
    "@types/benchmark": "1.0.31",
    "@types/chai": "4.1.4",
    "@types/faker": "4.1.2",
    "@types/joi": "13.4.4",
    "@types/mocha": "5.2.4",
    "@types/node": "10.1.2",
    "ajv": "6.5.2",
    "benchmark": "2.1.4",
    "c8": "3.2.1",
    "chai": "4.1.2",
    "faker": "4.1.0",
    "joi": "13.6.0",
    "mocha": "5.2.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.3"
  },
  "dependencies": {}
}
