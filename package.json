{
  "name": "jbq",
  "version": "0.3.0",
  "description": "JBQ - Yet another customizable JS schema validator.",
  "main": "lib.js",
  "scripts": {
    "tsc": "tsc -p ./tsconfig.dev.json",
    "tsc:watch": "tsc -w -p ./tsconfig.dev.json",
    "test": "mocha --require ts-node/register --project ./tsconfig.build_cjs.json ./test/main.test.ts",
    "test:cov": "c8 mocha --require ts-node/register ./test/main.test.ts",
    "test:perf": "node --require ts-node/register ./test/perf/benchmark.ts",
    "lint": "eslint src/**/*.ts test/**/*.ts examples/**/*.ts --fix"
  },
  "keywords": [
    "validator"
  ],
  "author": "krnik",
  "license": "ISC",
  "homepage": "https://github.com/krnik/jbq",
  "repository": {
    "type": "git",
    "url": "https://github.com/krnik/jbq"
  },
  "devDependencies": {
    "@types/benchmark": "1.0.31",
    "@types/chai": "4.1.7",
    "@types/faker": "4.1.5",
    "@types/mocha": "5.2.6",
    "@types/node": "12.0.2",
    "@types/pino": "5.8.7",
    "@types/prettier": "1.16.4",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "benchmark": "2.1.4",
    "c8": "5.0.1",
    "chai": "4.2.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-prettier": "3.1.0",
    "faker": "4.1.0",
    "mocha": "6.1.4",
    "pino": "5.12.6",
    "prettier": "1.17.1",
    "testcheck": "1.0.0-rc.2",
    "ts-node": "8.3.0",
    "typedoc": "0.14.2",
    "typedoc-plugin-markdown": "1.2.1",
    "typescript": "3.5.3"
  },
  "dependencies": {}
}
